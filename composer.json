{
    "name": "fab2s/searchable",
    "description": "Laravel searchable models based on FullText indexes",
    "type": "library",
    "authors": [{
        "name": "Fabrice de Stefanis"
    }],
    "support": {
        "issues": "https://github.com/fab2s/Searchable/issues",
        "source": "https://github.com/fab2s/Searchable"
    },
    "keywords": [
        "Laravel",
        "Searchable",
        "mysql",
        "FullText",
        "model"
    ],
    "license": [
        "MIT"
    ],
    "require": {
        "php": "^8.1",
        "fab2s/strings": "^1.0"
    },
    "require-dev": {
        "ext-pdo": "*",
        "phpunit/phpunit": "^10.0|^11.0",
        "laravel/pint": "^1.21.2",
        "orchestra/testbench": "^8.0|^9.0|^10.0"
    },
    "autoload": {
        "psr-4": {
            "fab2s\\Searchable\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "fab2s\\Searchable\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "fab2s\\Searchable\\SearchableServiceProvider"
            ]
        }
    },
    "scripts": {
        "post-update-cmd": [
            "rm -f .*.cache"
        ],
        "post-install-cmd": [
            "rm -f .*.cache"
        ],
        "fix": "@php vendor/bin/pint --config pint.json",
        "test": "@php vendor/bin/phpunit --",
        "cov": "@php vendor/bin/phpunit --coverage-html ./cov --"
    }
}
